extern "C"
{
 
  void OdomConsoleOutTestInit(RTC::Manager* manager)
  {
    coil::Properties profile(odomconsoleout_spec);
    manager->registerFactory(profile,
                             RTC::Create<OdomConsoleOutTest>,
                             RTC::Delete<OdomConsoleOutTest>);
  }
  
};


